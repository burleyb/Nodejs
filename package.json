		"eslint": "^9.9.1",
		"event-stream": "4.0.1",
		"extend": "3.0.2",
		"fast-csv": "^5.0.1",
		"fast-json": "^3.0.0",
		"flush-write-stream": "2.0.0",
		"glob": "^11.0.0",
		"ini": "^4.1.3",
		"leo-config": "1.1.0",
		"leo-logger": "1.0.1",
